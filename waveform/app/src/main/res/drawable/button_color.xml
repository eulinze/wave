<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/btn_stroke" />
            <solid android:color="@color/btn_press"></solid>
            <corners android:radius="5dp" />


        </shape>
    </item>
    <item >
        <shape>
            <stroke android:width="1dp" android:color="@color/btn_stroke" />
            <solid android:color="@color/btn_normal"></solid>
            <corners android:radius="5dp" />

        </shape>
    </item>



</selector>